<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns="http://castor.exolab.org/" xmlns:cst="http://castor.exolab.org/">
	
		<class name="com.thesaguaros.tumblrexpress.pojo.Tumblelog">
		<map-to xml="tumblelog" />
		
		<field name="title" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
		<field name="name" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
		<field name="url" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
		<field name="timezone" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
		<field name="avatarUrl16" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
		<field name="avatarUrl24" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
		<field name="avatarUrl30" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
		<field name="avatarUrl40" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
		<field name="avatarUrl48" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
		<field name="avatarUrl64" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
		<field name="avatarUrl96" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
		<field name="avatarUrl128" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
		<field name="avatarUrl512" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
	</class>
	
	<class name="com.thesaguaros.tumblrexpress.pojo.Post">
		<map-to xml="post" />
		<field name="tumblelog" type="com.thesaguaros.tumblrexpress.pojo.Tumblelog">
			<bind-xml auto-naming="deriveByClass" node="element" />
		</field>
	</class>

	<class name="com.thesaguaros.tumblrexpress.pojo.Posts">
		<map-to xml="posts" />
		<field name="posts" type="com.thesaguaros.tumblrexpress.pojo.Post"
			collection="arraylist">
			<bind-xml name="post" />
		</field>
	</class>
	
	<class name="com.thesaguaros.tumblrexpress.pojo.Dashboard">
		<map-to xml="tumblr" />
		<field name="version" type="java.lang.String">
			<bind-xml node="attribute" />
		</field>
		<field name="posts" type="com.thesaguaros.tumblrexpress.pojo.Posts">
			<bind-xml auto-naming="deriveByClass" node="element" />
		</field>		
	</class>
	
</mapping>